import pandas as pd
import numpy as np

dates = pd.date_range(start="2023-01-01", end="2025-02-14")

rows = []
for d in dates:
    day = d.day_name()
    is_weekend = day in ["Saturday", "Sunday"]
    weather = np.random.choice(["Sunny","Cloudy","Rainy"], p=[0.5,0.3,0.2])
    temperature = np.random.randint(22, 36)

    demand = 160
    if is_weekend:
        demand += 60
    if weather == "Rainy":
        demand -= 30
    if temperature > 32:
        demand += 20

    demand += np.random.randint(-25, 25)

    rows.append([d.date(), day, is_weekend, weather, temperature, demand])

df = pd.DataFrame(rows, columns=[
    "date","day","is_weekend","weather","temperature","orders"
])

df.head()
