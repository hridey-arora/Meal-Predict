import pandas as pd

future_results = pd.DataFrame({
    "date": pd.date_range(start="2025-02-15", periods=7),
    "day": ["Saturday","Sunday","Monday","Tuesday","Wednesday","Thursday","Friday"],
    "is_weekend": [True,True,False,False,False,False,False],
    "weather": ["Sunny","Cloudy","Sunny","Rainy","Sunny","Cloudy","Sunny"],
    "temperature": [32,30,29,28,30,31,33],
    "predicted_orders": [240,225,185,160,190,178,210]
})

future_results["recommended_prep"] = (future_results["predicted_orders"] * 0.9).astype(int)
future_results["waste_avoided"] = (
    future_results["predicted_orders"] - future_results["recommended_prep"]
)

future_results
